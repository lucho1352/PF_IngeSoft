@model FiestaFutbolera.VistaModelo.InformacionVM
<!DOCTYPE html>
<html>
<head>
    <title>Fiesta Futbolera SAS</title>
    <meta name="keywords" content="" />
    <meta name="description" content="" />
    <meta charset="utf-8">
    <meta name="viewport" content="initial-scale=1">

    <link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,700italic,800italic,400,300,600,700,800' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="../css/bootstrap.min.css">
    <link rel="stylesheet" href="../css/font-awesome.min.css">

    <link rel="stylesheet" href="../css/templatemo_misc.css">
    <link rel="stylesheet" href="../css/templatemo_style.css">
</head>
<body>

    <!-- This one in here is responsive menu for tablet and mobiles -->
    <div class="responsive-navigation visible-sm visible-xs">
        <a href="#" class="menu-toggle-btn">
            <i class="fa fa-bars fa-2x"></i>
        </a>
        <div class="navigation responsive-menu">
            <ul>
                <li class="home"><a href="#templatemo">Inicio</a></li>
                <li class="about"><a href="#about">NuestraEmpresa</a></li>
                <li class="services"><a href="#services">Servicios</a></li>
                <li class="portfolio"><a href="#portfolio">Reservas</a></li>
                <li class="ingreso"><a href="#ingreso">Ingreso</a></li>
                <li class="contact"><a href="#contact">Contacto</a></li>
            </ul> <!-- /.main_menu -->
        </div> <!-- /.responsive_menu -->
    </div> <!-- /responsive_navigation -->

    <div id="main-sidebar" class="hidden-xs hidden-sm">
        <div class="logo">
            <a href="#">
                <h1>Fiesta</h1>
                <h1>Futbolera</h1>
            </a>
        </div> <!-- /.logo -->

        <div class="navigation">
            <ul class="main-menu">
                <li class="home"><a href="#templatemo">Inicio</a></li>
                <li class="about"><a href="#about">Nuestra Empresa</a></li>
                <li class="services"><a href="#services">Servicios</a></li>
                <li class="portfolio"><a href="#portfolio">Reservas</a></li>
                <li class="ingreso"><a href="#ingreso">Ingreso</a></li>
                <li class="contact"><a href="#contact">Contactenos</a></li>
            </ul>
        </div> <!-- /.navigation -->
    </div> <!-- /#main-sidebar -->
    <!-- Inicio -->
    <div id="main-content">
        <div id="templatemo">
            <div class="main-slider">
                <div class="flexslider">
                    <ul class="slides">
                        <li>
                            <div class="slider-caption">
                                <h2>Deporte</h2>
                                <p>Reserve yá su cancha de futbol 5 y deje rodar el balón</p>
                                <!--<a href="#" class="largeButton homeBgColor">Read More</a>-->
                            </div>
                            <img src="../images/slide1.jpg" alt="Slide 1">
                        </li>
                        <li>
                            <div class="slider-caption">
                                <h2>Fiestas</h2>
                                <p>Lo invitamos a celebrar con nosotros sus mejores fiestas</p>
                                <!--<a href="#" class="largeButton homeBgColor">Details</a>-->
                            </div>
                            <img src="../images/slide2.jpg" alt="Slide 2">
                        </li>
                        <li>
                            <div class="slider-caption">
                                <h2>...y, mucho mas...</h2>
                                <p>Tenemos lo que esta buscando</p>
                                <!--<a href="#" class="largeButton homeBgColor">Downloads</a>-->
                            </div>
                            <img src="../images/slide3.jpg" alt="Slide 3">
                        </li>
                    </ul>
                </div>
            </div>
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <div class="welcome-text">
                            <h2>Bienvenido a Fiesta Futbolera</h2>
                            <p>Realice la mejor jugada de su vida e inverta en su salud, ya que al invertir en deporte invierte en su salud</p>
                        </div>
                    </div>
                </div>
            </div>
        </div> <!-- /#sTop -->
        <!-- Nuestra Empresa -->
        <div class="container-fluid">
            <div id="about" class="section-content">
                <div class="row">
                    <div class="col-md-12">
                        <div class="section-title">
                            <h2>Nuestra Empresa</h2>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        <div class="member-item">
                            <div class="member-thumb">
                                <img src="../images/Logo.jpg" alt="Logo">
                                <div class="overlay">
                                    <ul class="social-member">
                                        <li><a href="https://www.facebook.com/" class="fa fa-facebook"></a></li>
                                        <li><a href="https://twitter.com/?lang=es" class="fa fa-twitter"></a></li>
                                        <li><a href="https://co.linkedin.com/" class="fa fa-linkedin"></a></li>
                                    </ul>
                                </div>
                            </div>
                            <div class="member-content">
                                <h4>Logo</h4>
                                <p>Una imagen vale mas que mil palabras</p>
                            </div>
                        </div>
                    </div> <!-- /.col-md-4 -->
                    <div class="col-md-4">
                        <div class="member-item">
                            <div class="member-thumb">
                                <img src="../images/Cancha.jpg" alt="Cancha">
                                <div class="overlay">
                                    <ul class="social-member">
                                        <li><a href="https://www.facebook.com/" class="fa fa-facebook"></a></li>
                                        <li><a href="https://twitter.com/?lang=es" class="fa fa-twitter"></a></li>
                                        <li><a href="https://co.linkedin.com/" class="fa fa-linkedin"></a></li>
                                    </ul>
                                </div>
                            </div>
                            <div class="member-content">
                                <h4>Canchas</h4>
                                <p>Pensada para divertirte</p>
                            </div>
                        </div>
                    </div> <!-- /.col-md-4 -->
                    <div class="col-md-4">
                        <div class="member-item">
                            <div class="member-thumb">
                                <img src="../images/Recepcion.jpg" alt="Recepcion">
                                <div class="overlay">
                                    <ul class="social-member">
                                        <li><a href="https://www.facebook.com/" class="fa fa-facebook"></a></li>
                                        <li><a href="https://twitter.com/?lang=es" class="fa fa-twitter"></a></li>
                                        <li><a href="https://co.linkedin.com/" class="fa fa-linkedin"></a></li>
                                    </ul>
                                </div>
                            </div>
                            <div class="member-content">
                                <h4>Recepción</h4>
                                <p>La mejor barra para celebrar</p>
                            </div>
                        </div>
                    </div> <!-- /.col-md-4 -->
                </div> <!-- /.row -->
                <div class="row our-story">
                    <div class="col-md-8">
                        <h3>Misi&oacute;n</h3>
                        <p align="justify">Brindar a nuestros distinguidos clientes unas instalaciones adecuadas con el prop&oacute;posito de alcanzar un sano esparcimiento, basados en un excelente servicio, calidez y consecuci&oacute;n de sus expectativas.</p>
                        <h3></h3>
                        <h3>Visi&oacute;n</h3>
                        <p align="justify">Fiesta Futbolera es un espacio competitivo para el deporte, la recreaci&oacute; y ecenteos, siendo para el 2018 una organizaci&oacute;reconocida a nivel educativo, empresarial, social y comunitario en sus escenarios.</p>
                    </div>
                    <div class="col-md-4">
                        <div class="story-image">
                            <img src="../images/responsive-design.jpg" alt="">
                        </div>
                    </div>
                </div> <!-- /.row -->
            </div> <!-- /#about -->
            <!-- Servicios -->
            <div id="services" class="section-content">
                <div class="row">
                    <div class="col-md-12">
                        <div class="section-title">
                            <h2>Servicios</h2>
                        </div> <!-- /.section-title -->
                    </div> <!-- /.col-md-12 -->
                </div> <!-- /.row -->
                <div class="row">
                    <div class="col-md-4">
                        <div class="service-item clearfix">
                            <div class="service-icon">
                                <i class="fa fa-bolt fa-2x"></i>
                            </div>
                            <div class="service-content">
                                <h3>Eventos<br>Los más rápidos organizandolos</h3>
                            </div>
                        </div> <!-- /.service-item -->
                    </div> <!-- /.col-md-4 -->
                    <div class="col-md-4">
                        <div class="service-item">
                            <div class="service-icon">
                                <i class="fa fa-laptop fa-2x"></i>
                            </div>
                            <div class="service-content">
                                <h3>Reservas<br>Agendamos su reserva</h3>
                            </div>
                        </div> <!-- /.service-item -->
                    </div> <!-- /.col-md-4 -->
                    <div class="col-md-4">
                        <div class="service-item">
                            <div class="service-icon">
                                <i class="fa fa-pencil fa-2x"></i>
                            </div>
                            <div class="service-content">
                                <h3>Charla<br>Tómese una pola, y porqu&eacute; no otra </h3>
                            </div>
                        </div> <!-- /.service-item -->
                    </div> <!-- /.col-md-4 -->
                </div> <!-- /.row -->
                <div class="row our-skills">
                    <div class="col-md-8">
                        <h3>Habilidades</h3>
                        <p align="justify">...Chachara...</p>
                    </div>
                    <div class="col-md-4">
                        <ul class="progess-bars">
                            <li>
                                <div class="progress">
                                    <div class="progress-bar" role="progressbar" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100" style="width: 85%;">Reservas 85%</div>
                                </div>
                            </li>
                            <li>
                                <div class="progress">
                                    <div class="progress-bar" role="progressbar" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100" style="width: 90%;">Eventos 90%</div>
                                </div>
                            </li>
                            <li>
                                <div class="progress">
                                    <div class="progress-bar" role="progressbar" aria-valuenow="66" aria-valuemin="0" aria-valuemax="100" style="width: 95%;">Rumba y Pola 95%</div>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </div> <!-- /#services -->
<!-- Reservas -->
            <div id="portfolio" class="section-content">
                <div class="row">
                    <div class="col-md-12">
                        <div class="section-title">
                            <h2>Reservas</h2>
                        </div> <!-- /.section-title -->
                    </div> <!-- /.col-md-12 -->
                </div> <!-- /.row -->
                <div class="row">
                    <div class="col-md-3">
                        <label for="FechaReserva" class="required">Fecha Reserva:</label>
                    </div>
                    <div class="col-md-7">
                        <input name="FechaReserva" type="date" required="required">
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-3">
                        <label for="HoraReserva" class="required">Hora Reserva:</label>
                    </div>
                    <div class="col-md-7">
                        <input name="HoraReserva" type="time" min="9:00" max="22:00" required pattern="[9-22]{2}:[0]{2}">
                    </div>
                </div>
                    <div class="row">
                        <!-- Formulario -->
                        <div class="col-md-3">
                            <label for="NroID" class="required">Número Identificación:</label>
                        </div>
                        <div class="col-md-7">
                            @Html.TextBoxFor(model => model.Usuarios.NroIdUsuario)
                        </div>
                        <!--<input name="NroID" type="text" id="NroID" maxlength="30"></div>-->
                    </div>
                    <div class="row">
                        <div class="col-md-3">
                            <label for="TipoId" class="required">Tipo Identificacion:</label>
                        </div>
                        <div class="col-md-7">
                            <!--Html.TextBoxFor(model => model.Usuarios.TipoIdUsuario) </div> -->
                            @Html.DropDownListFor(model => model.Usuarios.TipoIdUsuario, new SelectList(
                  new List<Object>{
                       new { value = "CC" , text = "Cedula"  },
                       new { value = "TI" , text = "Tarjeta" },
                       new { value = "P" , text = "Passaporte" },
                       new { value = "N" , text = "Nit"}
                    },
                  "value",
                  "text",
                   1))
                        </div>
                    </div>
                    <div class="row">
                        <!--<input name="TipoId" type="text" id="TipoId" maxlength="30"></div>-->
                        <div class="col-md-3">
                            <label for="Nombre" class="required">Nombre:</label>
                        </div>
                        <div class="col-md-7">
                            @Html.TextBoxFor(model => model.Usuarios.Nombre)
                        </div>
                    </div>
                    <div class="row">
                        <!--<input name="Nombre" type="text" id="Nombre" maxlength="30"></div>-->
                        <div class="col-md-3">
                            <label for="Telefono" class="required">Teléfono contacto:</label>
                        </div>
                        <div class="col-md-7">
                            @Html.TextBoxFor(model => model.Usuarios.Telefono)
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="load-more">
                                <a href="#portfolio" class="largeButton portfolioBgColor">Reservar</a>
                            </div>
                        </div>
                    </div>
                </div> <!-- /#portfolio -->
            <!-- Ingreso -->
@using (Html.BeginForm("BuscarUsuarioRegistrado", "Home"))
            {
                <div id="ingreso" class="section-content">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="section-title">
                                <h2>Ingreso</h2>
                            </div> <!-- /.section-title -->
                        </div> <!-- /.col-md-12 -->
                    </div> <!-- /.row -->
                    <div class="row">
                        <div class="col-md-4">
                        </div> <!-- columna vacia -->
                        <div class="col-md-4">
                            <div class="portfolio-item">
                                <div class="portfolio-thumb">
                                    <img src="../images/portfolio/usuario.jpg" alt="Dragonfly">
                                    <div class="overlay-p">
                                        <a href="../images/portfolio/usuario.jpg" data-gal="prettyPhoto">
                                            <i class="fa fa-arrows-alt fa-2x"></i>
                                        </a>
                                    </div>
                                </div> <!-- /.portfolio-thumb -->
                                <h3 class="portfolio-title"><a href="#">Jugador</a></h3>
                                <div class="col-md-4">
                                    <label for="Nombre" class="required">Email:</label>
                                </div>
                                <div class="col-md-6">
                                    @Html.TextBoxFor(model1 => model1.Registrado.DirCorreo)
                                </div>
                                <!--<input name="Nombre" type="email" id="Nombre" maxlength="30"></div>-->
                                <div class="col-md-4">
                                    <label for="Password" class="required">Password:</label>
                                </div>
                                <div class="col-md-6">
                                    @Html.PasswordFor(model1 => model1.Registrado.Password)
                                </div>
                                <!--<input name="Password" type="password" id="Password" maxlength="30"></div>-->
                            </div>
                            <!-- /.portfolio-item -->
                        </div> <!-- /.col-md-4 -->
                    </div> <!-- /.row -->
                    <div class="row">
                        <div class="col-md-12">
                            <div class="load-more">
                                <button class="largeButton portfolioBgColor" type="submit" >Ingresar</button>
                                <a class="largeButton portfolioBgColor" @Html.ActionLink("Registro", "RegistroUsuario", "Home")></a>
                            </div>
                        </div>
                    </div>
                </div> <!-- /#portfolio -->
            }
            @using (Html.BeginForm("ContactoEmail", "Home"))
            {<!-- Contactenos -->
                <div id="contact" class="section-content">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="section-title">
                                <h2>Contactenos</h2>
                            </div> <!-- /.section-title -->
                        </div> <!-- /.col-md-12 -->
                    </div> <!-- /.row -->
                    <div class="row">
                        <div class="col-md-12">
                            <div class="map-holder">
                                <div class="google-map-canvas" id="map-canvas">
                                </div>
                            </div> <!-- /.map-holder -->
                        </div> <!-- /.col-md-12 -->
                    </div> <!-- /.row -->
                    <div class="row contact-form">
                        <div class="col-md-4">
                            <label for="Nombre" class="required">Nombre:</label>
                            <input name="Nombre" type="text" id="Nombre" maxlength="40">
                        </div> <!-- /.col-md-4 -->
                        <div class="col-md-4">
                            <label for="Email" class="required">Email:</label>
                            <input name="Email" type="email" id="Email" maxlength="40">
                        </div> <!-- /.col-md-4 -->
                        <div class="col-md-4">
                            <label for="subject-id">Descripcion:</label>
                            <input name="Descripcion" type="text" id="Descripcion" maxlength="60">
                        </div> <!-- /.col-md-4 -->
                        <div class="col-md-12">
                            <label for="Mensaje" class="required">Mensaje:</label>
                            <textarea name="Mensaje" id="Mensaje" rows="6"></textarea>
                        </div> <!-- /.col-md-12 -->
                        <div class="col-md-12">
                            <div class="submit-btn">
                                <button type="submit" class="largeButton contactBgColor">Enviar Mensaje</button>
                            </div> <!-- /.submit-btn -->
                        </div> <!-- /.col-md-12 -->
                    </div>
                </div> <!-- /#contact -->
            }
        </div> <!-- /.container-fluid -->
        <div class="site-footer">
            <div class="first-footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="social-footer">
                                <ul>
                                    <li><a href="https://www.facebook.com/" class="fa fa-facebook"></a></li>
                                    <li><a href="https://twitter.com/?lang=es" class="fa fa-twitter"></a></li>
                                    <li><a href="https://co.linkedin.com/" class="fa fa-linkedin"></a></li>
                                </ul>
                            </div> <!-- /.social-footer -->
                        </div> <!-- /.col-md-12 -->
                    </div> <!-- /.row -->
                </div> <!-- /.container-fluid -->
            </div> <!-- /.first-footer -->
            <div class="bottom-footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-6">
                            <p class="copyright">
                                Copyright © 2017 <a href="#">Fiesta Futbolera</a>
                            </p>
                        </div> <!-- /.col-md-6 -->
                        <div class="col-md-6 credits">
                            <p><!-- Ingesoluciones --></p>
                        </div> <!-- /.col-md-6 -->
                    </div> <!-- /.row -->
                </div> <!-- /.container-fluid -->
            </div> <!-- /.bottom-footer -->
        </div> <!-- /.site-footer -->

    </div> <!-- /#main-content -->

    @if (TempData["testmsg"] != null)
    {
        @Html.Raw(TempData["testmsg"])
    }
    <!-- JavaScripts -->
    <script src="../js/jquery-1.10.2.min.js"></script>
    <script src="../js/jquery.singlePageNav.js"></script>
    <script src="../js/jquery.flexslider.js"></script>
    <script src="../js/jquery.prettyPhoto.js"></script>
    <script src="../js/custom.js"></script>
    <script>
		$(document).ready(function(){
			$("a[data-gal^='prettyPhoto']").prettyPhoto({hook: 'data-gal'});
		});

        function initialize()
		{
			var mapOptions = {
			zoom: 17,
			center: new google.maps.LatLng(4.6862625,-74.1430899)
			};

			var map = new google.maps.Map(document.getElementById('map-canvas'),mapOptions);

			marker = new google.maps.Marker({
			map: map,
			draggable: true,
			animation: google.maps.Animation.DROP,
			position: {lat: 4.6863211, lng: -74.1428313}});
        }

        function loadScript()
		{
			var script = document.createElement('script');
			script.type = 'text/javascript';
			script.src = 'https://maps.googleapis.com/maps/api/js?key=AIzaSyA7tm2Zl99G7_6C9PxIL-J07q5b3psmiKQ&callback=initialize';

			document.body.appendChild(script);
        }
        window.onload = loadScript;
    </script>
</body>
</html>